<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="WhiteCheck">
        <item name="colorAccent">#ffffff</item>
    </style>
    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/floatloginlabels</item>
        <item name="colorAccent">@color/white_color</item>
        <item name="colorControlNormal">@color/floatloginlabels</item>
        <item name="colorControlHighlight">@color/floatloginlabels</item>
        <item name="colorControlActivated">@color/floatloginlabels</item>
        <!-- <item name="android:textSize">20sp</item>-->
    </style>

</resources>
